<div class="modal fade" id="staticBackdrop" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1"
    aria-labelledby="staticBackdropLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <form class="edit_emp" id="zeroedit">
                <div class="popup_header">
                    <h4>Edit Employee</h4>
                    <button type="button" class="btn" data-bs-dismiss="modal"><i class="fa-solid fa-xmark"
                            style="color: #2B3674;"></i></button>
                </div>
                <div class="popup_body">
                    <div class="col top_col -lg-6 mt-3">
                        <div class="card-body edit_body">
                            <div class="edit_emp_img">
                                <img id="editImg" src="" alt="">
                            </div>
                            <!-- <input type="file" id="edit_epm_file" accept="image/*"> -->
                            <label for="edit_epm_file">
                                <button type="button" class="btn btn-light">Change</button>
                            </label>
                        </div>
                        <div class="row mt-3">
                            <div class=" col-md-3 mb-3 ">
                                <label for="exampleFormControlInput1" class="form-label">Salutation</label>
                                <select class="form-select" aria-label="Default select example" id="editsalutation">
                                    <option selected value="select">select</option>
                                    <option value="Mr">Mr.</option>
                                    <option value="Mrs">Mrs.</option>
                                </select>
                                <span for="errormessage" class="errormessage" id="editErrormessageSalutation"></span>
                            </div>
                            <div class=" col-md-5 mb-3">
                                <label for="exampleFormControlInput1" class="form-label">First Name</label>
                                <input type="text" class="form-control" id="editfirstName" placeholder="john">
                                <span for="errormessage" class="errormessage" id="editErrormessagefirstname"></span>
                            </div>
                            <div class=" col-md-4 mb-3">
                                <label for="exampleFormControlInput1" class="form-label">Last Name</label>
                                <input type="text" class="form-control" id="editlastName" placeholder="doe ">
                                <span for="errormessage" class="errormessage" id="editErrormessagelastname"></span>
                            </div>
                        </div>
                        <div class="row">
                            <div class=" col-md-6 mb-3">
                                <label for="exampleFormControlInput1" class="form-label">Email Address</label>
                                <input type="email" class="form-control" id="editemail"
                                    placeholder="mail@simmmple.com">
                                <span for="errormessage" class="errormessage" id="editErrormessageEmail"></span>
                            </div>
                            <div class=" col-md-6 mb-3">
                                <label for="exampleFormControlInput1" class="form-label">Mobile Number</label>
                                <input type="text" class="form-control" id="editmobilenumber"
                                    placeholder="enter your Mobile Number">
                                <span for="errormessage" class="errormessage" id="editErrormessagePhone"></span>
                            </div>
                        </div>
                        <div class="row">
                            <div class=" col-md-6 mb-3">
                                <label for="exampleFormControlInput1" class="form-label">Date of Birth</label>
                                <input type="date" class="form-control date" id="editdob"
                                    placeholder="select date of birth">
                                <span for="errormessage" class="errormessage" id="editErrormessageDob"></span>
                            </div>
                            <div class=" col-md-6 mb-3">
                                <label for="exampleFormControlInput1" class="form-label">Gender</label>
                                <div class="d-flex gap-4">
                                    <div class="d-flex gap-2 mt-2">
                                        <input class="form-check-input" type="radio" name="genders" value="male"
                                            aria-label="...">
                                        <label for="exampleFormControlInput1" class="form-label Gender">Male</label>
                                    </div>
                                    <div class="d-flex gap-2 mt-2">
                                        <input class="form-check-input" type="radio" name="genders" value="female"
                                            aria-label="...">
                                        <label for="exampleFormControlInput1" class="form-label Gender">Female</label>
                                    </div>
                                </div>
                            </div>
                            <span for="errormessage" class="errormessage" id="editErrormessageGender"></span>
                        </div>
                        <div class="row">
                            <div class=" col-md-12 mb-3">
                                <label for="exampleFormControlInput1" class="form-label">Qualifications</label>
                                <input type="text" class="form-control" id="editqualifications"
                                    placeholder="enter address">
                                <span for="errormessage" class="errormessage" id="editErrormessageQualifications"></span>
                            </div>
                        </div>
                        <div class="row">
                            <div class=" col-md-12 mb-3">
                                <label for="exampleFormControlInput1" class="form-label">Address</label>
                                <input type="text" class="form-control" id="editaddress" placeholder="enter address">
                                <span for="errormessage" class="errormessage" id="editErrormessageAddress"></span>
                            </div>
                        </div>
                        <div class="row">
                            <div class=" col-md-6 mb-3">
                                <label for="exampleFormControlInput1" class="form-label">Country</label>
                                <select class="form-select" aria-label="Default select example" id="editcountry"
                                    placeholder="select Country">
                                    <option selected value="select country">select country</option>
                                    <option value="India">India</option>
                                    <option value="Australia">Australia</option>
                                    <option value="Brazil">Brazil</option>
                                    <option value="China">China</option>
                                </select>
                                <span for="errormessage" class="errormessage" id="editErrormessageCountry"></span>
                            </div>
                            <div class=" col-md-6 mb-3">
                                <label for="exampleFormControlInput1" class="form-label">State</label>
                                <select class="form-select" aria-label="Default select example" id="editstate"
                                    placeholder="select state">
                                    <option selected>select state</option>
                                    <option>Kerala</option>
                                    <option>Tamilnadu</option>
                                    <option>Etc...</option>
                                </select>
                                <span for="errormessage" class="errormessage" id="editErrormessageState"></span>
                            </div>
                        </div>
                        <div class="row">
                            <div class=" col-md-6 mb-3">
                                <label for="exampleFormControlInput1" class="form-label">City</label>
                                <input type="text" class="form-control" id="editcity" placeholder="enter city">
                                <span for="errormessage" class="errormessage" id="editErrormessageCity"></span>
                            </div>
                            <div class=" col-md-6 mb-3">
                                <label for="exampleFormControlInput1" class="form-label">Pin/Zip</label>
                                <input type="text" class="form-control" id="editpin" placeholder="enter zip/pin">
                                <span for="errormessage" class="errormessage" id="editErrormessagePin"></span>
                            </div>
                        </div>
                        <div class="row">
                            <div class=" col-md-6 mb-3">
                                <label for="exampleFormControlInput1" class="form-label">Username</label>
                                <input type="text" class="form-control" id="editusername"
                                    placeholder="enter your username">
                                <span for="errormessage" class="errormessage" id="editErrormessageUsrname"></span>
                            </div>
                            <div class=" col-md-6 mb-3">
                                <label for="exampleFormControlInput1" class="form-label">Password</label>
                                <input type="password" class="form-control" id="editpassword"
                                    placeholder="enter your password">
                                <span for="errormessage" class="errormessage" id="editErrormessagePass"></span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="popup_footer">
                    <button type="button" class="btn btn-light" data-bs-dismiss="modal">Cancel</button>
                    <button type="submit" class="btn btn-primary">Save
                        Changes</button>
                </div>
            </form>
        </div>
    </div>
</div>