    <!-- bootstrap -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">
    <!-- bootstrap cdn  css-->
    <link rel="stylesheet" href="	https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css">
    <!--  -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js">
</head>

<body>
    <div class="box">
        <div class="employee_management">
            <div class="side_menu">
                <div class="mini_box">
                    <h2>Employee</h2>
                    <h3>Management</h3>
                </div>
                <div class="mini_box list">
                    <ul>
                        <li>
                            <div class="line"></div><a href="#"><i class="fa-solid fa-house"></i>Dashboard</a>
                        </li>
                        <li>
                            <div class="line"></div><a href="index.html"><i
                                    class="fa-solid fa-chart-simple"></i>Employees</a>
                        </li>
                        <li>
                            <div class="line"></div><a href="#"><i class="fa-solid fa-user"></i>My Profile</a>
                        </li>
                        <li>
                            <div class="line"></div><a href="#"><i class="fa-solid fa-lock"></i>Logout</a>
                        </li>
                    </ul>
                </div>
                <div class="mini_box footer">
                    <p>Powered by</p>
                    <div class="side_menu_img">
                        <img src="img/image 1.png" alt="">
                    </div>
                </div>
            </div>
            <div class="main_page">
                <!-- main_header -->
                <div class="main_header">
                    <div class="mini_box">
                        <h4>Dashboard / Employees</h4>
                        <h1>Employees</h1>
                    </div>
                    <div class="main_box_right">
                        <div class="main_input">
                            <div class="input">
                                <i class="fa-solid fa-magnifying-glass" style="color: #101032;"></i>
                                <input type="text" placeholder="Search">
                            </div>
                            <i class="fa-regular fa-bell main" style="color: #D3D8E2;"></i>
                            <div class="main_img">
                                <img src="img/Elipse 5.png" alt="">
                            </div>
                        </div>
                    </div>
                </div>

                <!-- main_section -->
                <div class="main_section">
                    <div class="box_view_section">
                        <div class="main_section_table_head">
                            <div class="view_img">
                                <img src="img/Background Image.png" alt="">
                            </div>
                            <div class="emp_view_img" id="viewEmployeeProfilePic">
                                <img alt="">
                            </div>
                            <div class="view_top">
                                <h3 id="employeeName"></h3>
                                <p id="employeeEmail"></p>
                            </div>

                            <div class="row mt-3 me-3 ms-3 ">
                                <!-- Gender -->
                                <div class=" col-md-3 view_col gen">
                                    <label for="exampleFormControlInput1" class="form-label">Gender</label>
                                    <div id="employeeGender"></div>
                                </div>
                                <!-- Age -->
                                <div class=" col-md-3 view_col age">
                                    <label for="exampleFormControlInput1" class="form-label">Age</label>
                                    <div id="employeeAge"></div>
                                </div>
                                <!-- Date of Birth -->
                                <div class=" col-md-3 view_col dob">
                                    <label for="exampleFormControlInput1" class="form-label">Date of Birth</label>
                                    <div id="employeeDob"></div>
                                </div>
                            </div>

                            <div class="row mt-3 me-3 ms-3 ">
                                <!-- Mobile Number -->
                                <div class=" col-md-3 view_col mob">
                                    <label for="exampleFormControlInput1" class="form-label">Mobile Number</label>
                                    <div id="employeePhone"></div>
                                </div>
                                <!-- Qualifications -->
                                <div class=" col-md-3 view_col qua">
                                    <label for="exampleFormControlInput1" class="form-label">Qualifications</label>
                                    <div id="employeeQua"></div>
                                </div>
                            </div>

                            <div class="row mt-3 me-3 ms-3 ">
                                <!-- Address -->
                                <div class=" col-md-3 view_col mob">
                                    <label for="exampleFormControlInput1" class="form-label">Address</label>
                                    <div id="employeeAddress"></div>
                                </div>
                                <!-- Username -->
                                <div class=" col-md-3 view_col qua">
                                    <label for="exampleFormControlInput1" class="form-label">Username</label>
                                    <div id="employeeUsername"></div>
                                </div>
                            </div>
                            <div class="btn_view" id="btn_view">
                                <button type="button" class="btn btn-dark" onclick=delete_emp()>Delete</button>
                                <button type="button" class="btn btn-primary">Edit Details</button>
                            </div>
                        </div>

                        <!-- main footer -->
                        <div class="main_footer">
                            <div class="footer_bx">
                                <p>Â© 2023 All Rights Reserved. Stackup</p>
                                <div class="main_footer_right">
                                    <p>Terms of Use</p>
                                    <p>Privacy Policy</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- javascript link -->
            <script src="/js/index.js"></script>
            <script src="/js/view.js"></script>
            <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"
                integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM"
                crossorigin="anonymous"></script>
</body>
</html>